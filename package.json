{
  "name": "cranyons",
  "version": "1.2.1-11",
  "description": "Choose your own pic adventure Angular 1.x SPA",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "karma start config/karma.conf.js --no-auto-watch --single-run --reporters spec",
    "prebuild": "rimraf public",
    "build": "webpack --config config/webpack.dev.config.js --progress --colors --display-error-details",
    "prebuild:prod": "rimraf public",
    "build:prod": "webpack --config config/webpack.production.config.js --progress --colors --display-error-details",
    "update:index": "babel-node internal/find-and-replace-gzip.js",
    "upload": "babel-node internal/upload.js",
    "deploy": "npm run build:prod && npm run update:index && npm run upload",
    "image:build": "sudo docker build -f ../deployment/cranyons/Dockerfile -t deanslamajr/cranyons:\"$(git describe)\" .",
    "image:promote": "sudo docker push deanslamajr/dslama.net:\"$(git describe)\"",
    "image:start": "sudo docker run --env-file ./.env -p 8080:8080/tcp deanslamajr/cranyons:\"$(git describe)\"",
    "publish": "npm run image:build",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.5",
    "axios": "^0.12.0",
    "babel-polyfill": "^6.5.0",
    "body-parser": "^1.12.4",
    "ejs": "^2.3.1",
    "express": "^4.12.4",
    "mongojs": "^2.4.0",
    "nconf": "^0.8.5",
    "svgjs": "^2.2.5"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "aws-sdk": "^2.184.0",
    "axios-mock-adapter": "^1.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "ignore-loader": "^0.1.1",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "object.values": "^1.0.4",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "replace-in-file": "^3.1.0",
    "rimraf": "^2.5.2",
    "s3-upload-stream": "^1.0.7",
    "sinon": "^1.17.5",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.1"
  },
  "author": "Dean Slama",
  "license": "ISC"
}
